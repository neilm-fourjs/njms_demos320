		<ENVIRONMENT_VARIABLE Id="GREOUTPUTDIR">/tmp</ENVIRONMENT_VARIABLE>
		<!--ENVIRONMENT_VARIABLE Id="GDCUPDATEDIR">..$(sep)..$(sep)gdcupdate</ENVIRONMENT_VARIABLE-->
		<!--ENVIRONMENT_VARIABLE Id="GDCUPDATESERVER">http://localhost/g/ws/r</ENVIRONMENT_VARIABLE-->
